<%- include('../partials/header.ejs') %>

    <!-- Sidebar Start -->
    <%- include('../partials/adminSidebar.ejs') %>
        <!-- Sidebar End -->


        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->

            <%- include('../partials/navbar.ejs') %>
                <!-- Navbar End -->


                <!-- Container -->
                <div class="container-fluid pt-4 px-4">
                    <div class="fw-bold mb-2 fs-3">Student Details :</div>
                    <div class="row g-4">
                        <div class="col-12">
                            <div class="bg-light rounded p-4">
                                <!-- view start -->
                                <% if (locals.foundAdmForm) { %>
                                    <form class="row g-3">

                                        <div class="row">
                                            <div class="col-md-4">
                                                <img src=<%=locals.foundAdmForm.studentPhoto%> alt="Image 1"
                                                class="img-fluid">
                                                <div class="fw-bold text-center mt-3">Student Photo</div>
                                            </div>
                                            <div class="col-md-4">
                                                <img src=<%=locals.foundAdmForm.studentSign %> alt="Image 2"
                                                class="img-fluid">
                                                <div class="fw-bold text-center mt-3">Student Sign</div>
                                            </div>
                                            <div class="col-md-4">
                                                <img src=<%=locals.foundAdmForm.paymentSS%> alt="Image 3"
                                                class="img-fluid">
                                                <div class="fw-bold text-center mt-3">Payment Screenshot</div>
                                            </div>
                                        </div>

                                        <div class="col-md-6">
                                            <label for="" class="form-label fw-bold">Student's Name</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.studentName%></textarea>
                                        </div>

                                        <div class="col-md-6">
                                            <label for="" class="form-label fw-bold">Father's Name</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.fatherName%></textarea>
                                        </div>

                                        <div class="col-md-6">
                                            <label for="" class="form-label fw-bold">Mother's Name</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.motherName%></textarea>
                                        </div>

                                        <div class="col-md-6">
                                            <label for="" class="form-label fw-bold">Guardian's Name</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.guardianName%></textarea>
                                        </div>
                                        <hr>
                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Reference Number</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.referenceNumber%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">PPU Confidential Number</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.ppuConfidentialNumber%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Applicant Id</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.applicantId%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">College Roll Number</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.collegeRollNo ? locals.foundAdmForm.collegeRollNo.slice(2) : "NA" %></textarea>
                                        </div>

                                        <div class="col-md-6">
                                            <label for="" class="form-label fw-bold">Course</label>
                                            <% if (locals.foundStudent) { %>
                                            <textarea rows="1" class="form-control" readonly><%= locals.foundStudent.course%></textarea>
                                            <% } else { %>
                                                <textarea rows="1" class="form-control" readonly>NA</textarea>
                                            <% } %>
                                        </div>

                                        <div class="col-md-6">
                                            <label for="" class="form-label fw-bold">Email</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.email%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Paper 1 / MJC-1</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.paper1%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Paper 2 / MIC-1</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.paper2%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Paper 3 / MDC-1</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.paper3%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Paper 4 / AEC-1</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.paper4%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Paper 5 / SEC-1</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.paper5%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Paper 6 / VAC-1</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.paper6%></textarea>
                                        </div>
                                        <hr>
                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Date of Birth</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.dOB%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Gender</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.gender%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Category</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.category%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Religion</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.religion%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Family Annual Income</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.familyAnnualIncome%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Blood Group</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.bloodGroup%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Physically Challenged</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.physicallyChallenged%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Marital Status</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.maritalStatus%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Applicant Aadhar No</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.aadharNumber%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Mobile Number</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.mobileNumber%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">WhatsApp Number</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.whatsAppNumber%></textarea>
                                        </div>
                                        <hr>
                                        <div class="col-md-8">
                                            <label for="" class="form-label fw-bold">Address</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.address%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">District</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.district%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Police Station</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.policeStation%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">State</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.state%></textarea>
                                        </div>

                                        <div class="col-md-4">
                                            <label for="" class="form-label fw-bold">Pin Code</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.pinCode%></textarea>
                                        </div>

                                        <hr>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Last Exam Name</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.examName%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Last Exam Board</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.examBoard%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Last Exam Year</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.examYear%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Last Exam Result</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.examResult%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Last Exam Obt. Marks /
                                                Grade</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.obtMarks%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Last Exam Full Marks /
                                                Grade</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.fullMarks%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Last Exam Obt Percentage</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.obtPercent%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Session</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.session%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Admission fee</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.admissionFee%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Payment ID</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.paymentId%></textarea>
                                        </div>

                                        <div class="col-md-3">
                                            <label for="" class="form-label fw-bold">Receipt No</label>
                                            <textarea rows="1" class="form-control"
                                                readonly><%= locals.foundAdmForm.receiptNo%></textarea>
                                        </div>
                                    </form>
                                    <% } %>

                                        <!-- view end -->

                            </div>
                            
                            <a href="/ug-reg-sem-1-25-29-editStudentDetails/<%= locals.foundAdmForm._id %>"
                                class="px-4 btn btn-warning w-md">Edit</a>
                        </div>

                    </div>
                </div>
                <!-- Sale & Revenue End -->


                <!-- Footer Start -->
                <div class="container-fluid pt-4 px-4">
                    <div class="bg-light rounded-top p-4">
                        <div class="row">
                            <div class="col-12 col-sm-6 text-center text-sm-start">
                                &copy; <a href="#">Maltidhari College Naubatpur, Patna</a>, All Right Reserved.
                            </div>
                            <div class="col-12 col-sm-6 text-center text-sm-end">
                                Distributed By <a href="#">Digital India</a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Footer End -->
        </div>
        <!-- Content End -->


        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
        </div>

        <!-- JavaScript Libraries -->
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
        <script src="../lib/chart/chart.min.js"></script>
        <script src="../lib/easing/easing.min.js"></script>
        <script src="../lib/waypoints/waypoints.min.js"></script>
        <script src="../lib/owlcarousel/owl.carousel.min.js"></script>
        <script src="../lib/tempusdominus/js/moment.min.js"></script>
        <script src="../lib/tempusdominus/js/moment-timezone.min.js"></script>
        <script src="../lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

        <!-- Template Javascript -->
        <script src="../javascripts/main.js"></script>
        <script>
            const goBackBtn = document.querySelector('.goBackBtn')
            goBackBtn.addEventListener('click', () => {
                window.history.back()

            })
        </script>
        </body>

        </html>