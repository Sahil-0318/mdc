        { value: 'Economics 326', text: 'Economics 326' },
        { value: 'Mathematics 327', text: 'Mathematics 327' }
    ];
    
    // Initialize the first select with subjects
    const electiveSubject1 = document.getElementById('electiveSubject1');
    subjects.forEach(subject => {
        const option = document.createElement('option');
        option.value = subject.value;
        option.textContent = subject.text;
        electiveSubject1.appendChild(option);
    });
    
    // Function to update the second select based on the first selection
    function updateSecondSelect() {
        const firstSubject = document.getElementById('electiveSubject1').value;
        const electiveSubject2 = document.getElementById('electiveSubject2');
        electiveSubject2.innerHTML = '<option value="">-- Select Elective Subject 2 --</option>'; // Reset the second select options
    
        // Filter out the selected subject and populate second select
        const remainingSubjects = subjects.filter(subject => subject.value !== firstSubject);
        remainingSubjects.forEach(subject => {
            const option = document.createElement('option');
            option.value = subject.value;
            option.textContent = subject.text;
            electiveSubject2.appendChild(option);
        });
    
        // Reset the third select
        document.getElementById('electiveSubject3').innerHTML = '<option value="">-- Select Elective Subject 3 --</option>';
    }
    
    // Function to update the third select based on the second selection
    function updateThirdSelect() {
        const firstSubject = document.getElementById('electiveSubject1').value;
        const secondSubject = document.getElementById('electiveSubject2').value;
        const electiveSubject3 = document.getElementById('electiveSubject3');
        electiveSubject3.innerHTML = '<option value="">Select a subject</option>'; // Reset the third select options
    
        // Filter out the selected subjects and populate third select
        const remainingSubjects = subjects.filter(subject => subject.value !== firstSubject && subject.value !== secondSubject);
        remainingSubjects.forEach(subject => {
            const option = document.createElement('option');
            option.value = subject.value;
            option.textContent = subject.text;
            electiveSubject3.appendChild(option);
        });
    }
    </script>

