     <script>
    // Array of subjects with their codes
    const subjects = [
        { value: 'Music 318', text: 'Music 318' },
        { value: 'Home Science 319', text: 'Home Science 319' },
        { value: 'Philosophy 320', text: 'Philosophy 320' },
